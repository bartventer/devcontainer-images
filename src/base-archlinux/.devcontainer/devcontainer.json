{
  "build": {
    "dockerfile": "./Dockerfile",
    "context": ".",
    "args": {
      "VARIANT": "latest"
    }
  },
  "features": {
    "ghcr.io/bartventer/arch-devcontainer-features/common-utils": {
      "configureZshAsDefaultShell": true,
      "installOhMyZshConfig": false,
      "additionalPackages": "nodejs npm yarn make jq go-yq"
    }
  },
  "remoteUser": "vscode"
}